<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cuit.hyl.graduation.fastdfs.dao.FileUploadDao">

    <resultMap type="cuit.hyl.graduation.fastdfs.entity.FileUpload" id="FileUploadMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
        <result property="filePath" column="file_path" jdbcType="VARCHAR"/>
        <result property="uploadTime" column="upload_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--新增列-->
    <insert id="insert">
        insert into graduation_ui.file_upload(id, file_name, file_path)
        values (#{id}, #{fileName}, #{filePath})
    </insert>

</mapper>